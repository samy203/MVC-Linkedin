@model NavigateViewModel
<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" id="bootstrap-css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/stylesheet.css">
</head>



<body>

    <nav class="navbar  navbar-icon-top navbar-expand-lg  navbar-dark ">
        <!-- <a class="navbar-brand" href="#">Navbar</a> -->
        <a href="#" class="fa fa-linkedin-square navbar-brand" aria-hidden="true"></i></a>

        <form class="navbar-form" role="search">
            <div class="input-group">
                @Html.TextBoxFor(m => m.SearchIndex, new { @class = "form-control", placeholder="Search" })
                <div class="search-btn">
                    <a class="btn btn-default ffs" type="submit" href="@Url.Action("index","Search",Model)"><i class="fa fa-search" style="padding-top:4px"></i></a>
                </div>
            </div>
        </form>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav">
                <li class="nav-item">

                    <a class="nav-link" href="@Url.Action("Index", "Feed", Model)">
                        <i class="fa fa-home icon"></i><br>
                        <span class="icon-text">Home</span>
                    </a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link" href="@Url.Action("Index","Connections",Model)">
                        <i class="fa fa-users icon"></i>
                        <span class="icon-text">Network</span>
                    </a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link" href="#">
                        <i class="fa fa-suitcase icon"></i><br>
                        <span class="icon-text">Jobs</span>
                    </a>
                </li>
                <li class="nav-item ">
                    <a class="nav-link disabled" href="#">
                        <i class="fa fa-envelope-o icon">
                            <span class="badge badge-danger">11</span>
                        </i>
                        <span class="icon-text">Messages</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">
                        <i class="fa fa-globe icon">
                            <span class="badge badge-info">11</span>
                        </i>
                        <span class="icon-text">Notifications</span>
                    </a>
                </li>
                <span class="spacer"></span>





                <li class="nav-item">
                    <a class="nav-link" href="@Url.Action("Index","Person",Model)">
                        <i class="fa fa-user icon"></i><br>
                        <span class="icon-text">profile</span>
                    </a>
                </li>





                <li class="nav-item">
                    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm", @class = "navbar-right" }))
                    { 
                        @Html.AntiForgeryToken()
                        <a class="nav-link" href="javascript:document.getElementById('logoutForm').submit()">
                            <i class="fa fa-sign-out icon"></i><br>
                            <span class="icon-text">Sign-out</span>
                        </a>
                    }
                    </li>
                </ul>
            </div>
        </nav>


        @*@Scripts.Render("~/bundles/profileFnc")*@
                        @RenderBody()


                    </body>










